package game.Model;

import java.awt.Image;

public class Shot {
	
	private Image image;
	private int x, y;
	private int width, height;
	private boolean isVisible;
	
	
	private static final int WIDTH = 938;
	private static int SPEED = 2;
	
	
	public Shot(int x, int y) {
		this.x = x;
		this.y = y;
		isVisible = true;
	}
	
	public void load() {
		ImageIcon reference = new ImageIcon();
	}
}
